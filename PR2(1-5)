public class Main {
        public static void main(String[] args) {
            // Задаем массив чисел
            int[] array = {5, 6, 13, 14, 24};

            // Вычисляем сумму чисел с помощью цикла for
            int sumFor = 0;
            for (int i = 0; i < array.length; i++) {
                sumFor = sumFor + array[i];
            }
            System.out.println("Сумма чисел с помощью цикла for: " + sumFor);

            // Вычисляем сумму чисел с помощью цикла while
            int sumWhile = 0;
            int j = 0;
            while (j < array.length) {
                sumWhile = sumWhile + array[j];
                j++;
            }
            System.out.println("Сумма чисел с помощью цикла while: " + sumWhile);

            // Вычисляем сумму чисел с помощью цикла do while
            int sumDoWhile = 0;
            int k = 0;
            do {
                sumDoWhile = sumDoWhile + array[k];
                k++;
            } while (k < array.length);
            System.out.println("Сумма чисел с помощью цикла do while: " + sumDoWhile);
        }
}
